<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/firstFragment">

    <!-- First Fragment -->
    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.projectimage.fragment.SelCatFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first">
        <action
            android:id="@+id/action_firstFragment_to_secondFragment"
            app:destination="@id/secondFragment" />
    </fragment>

    <!-- Second Fragment -->
    <fragment
        android:id="@+id/secondFragment"
        android:name="com.example.projectimage.fragment.AgeFragment"
        android:label="fragment_second"
        tools:layout="@layout/fragment_second">
        <action
            android:id="@+id/action_secondFragment_to_thirdFragment"
            app:destination="@id/thirdFragment" />
    </fragment>

    <!-- Third Fragment -->
    <fragment
        android:id="@+id/thirdFragment"
        android:name="com.example.projectimage.fragment.ConfirmFragment"
        android:label="fragment_third"
        tools:layout="@layout/fragment_third">
        <action
            android:id="@+id/action_thirdFragment_to_fourthFragment"
            app:destination="@id/fourthFragment" />
    </fragment>

    <!-- Fourth Fragment -->
    <fragment
        android:id="@+id/fourthFragment"
        android:name="com.example.projectimage.fragment.AssesmentFragment"
        android:label="fragment_fourth"
        tools:layout="@layout/fragment_fourth">
        <action
            android:id="@+id/action_fourthFragment_to_fifthFragment"
            app:destination="@id/fifthFragment" />
    </fragment>

    <!-- Fifth Fragment -->
    <fragment
        android:id="@+id/fifthFragment"
        android:name="com.example.projectimage.fragment.LispFragment"
        android:label="fragment_fifth"
        tools:layout="@layout/fragment_fifth">
        <action
            android:id="@+id/action_fifthFragment_to_sixFragment"
            app:destination="@id/sixFragment" />

    </fragment>

    <!-- Six Fragment -->
    <fragment
        android:id="@+id/sixFragment"
        android:name="com.example.projectimage.fragment.SoundFragment"
        android:label="fragment_six"
        tools:layout="@layout/fragment_six">
        <action
            android:id="@+id/action_sixFragment_to_NextActivity" />
    </fragment>

</navigation>
